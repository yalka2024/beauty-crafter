{
  "name": "beauty-crafter",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@4.2.2",
  "workspaces": [
    "New folder/BeautyCrafterMobile",
    "lib"
  ],
  "scripts": {
    "build": "prisma generate && next build",
    "dev": "next dev",
    "start": "next start",
    "start:production": "node scripts/start-production.js",
    "start:server": "node server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:integration": "jest --testPathPatterns=integration",
    "test:security": "jest --testPathPatterns=security",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "load-test": "node scripts/load-test-simple.js",
    "load-test:100": "cross-env CONCURRENT_USERS=100 node scripts/load-test-simple.js",
    "load-test:500": "cross-env CONCURRENT_USERS=500 node scripts/load-test-simple.js",
    "load-test:1000": "cross-env CONCURRENT_USERS=1000 node scripts/load-test-simple.js",
    "load-test:optimized": "node scripts/load-test-optimized.js",
    "load-test:optimized:100": "cross-env CONCURRENT_USERS=100 node scripts/load-test-optimized.js",
    "load-test:optimized:500": "cross-env CONCURRENT_USERS=500 node scripts/load-test-optimized.js",
    "load-test:optimized:1000": "cross-env CONCURRENT_USERS=1000 node scripts/load-test-optimized.js",
    "load-test:production": "node scripts/load-test-production.js",
    "load-test:production:100": "cross-env CONCURRENT_USERS=100 node scripts/load-test-production.js",
    "load-test:production:500": "cross-env CONCURRENT_USERS=500 node scripts/load-test-production.js",
    "load-test:production:1000": "cross-env CONCURRENT_USERS=1000 node scripts/load-test-production.js",
    "pen-test": "node scripts/penetration-test.js",
    "pen-test:verbose": "VERBOSE=true node scripts/penetration-test.js",
    "security:test": "npm run pen-test && npm run load-test:100",
    "test:all": "node scripts/run-all-tests.js",
    "test:comprehensive": "VERBOSE=true node scripts/run-all-tests.js",
    "test:production": "SKIP_DATABASE=true node scripts/run-all-tests.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "security:audit": "npm audit && npm audit fix",
    "security:check": "snyk test",
    "build:analyze": "ANALYZE=true npm run build",
    "docker:build": "docker build -t beauty-crafter .",
    "docker:run": "docker run -p 3000:3000 beauty-crafter",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "workspaces:list": "yarn workspaces list",
    "workspaces:foreach": "yarn workspaces foreach run build",
    "workspaces:upgrade": "yarn workspaces foreach npm update",
    "ci:lint": "yarn lint && yarn type-check",
    "ci:test": "yarn test:ci",
    "ci:build": "yarn build",
    "ci:all": "yarn ci:lint && yarn ci:test && yarn ci:build",
    "release": "standard-version",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "monitor:performance": "node scripts/performance-monitor.js",
    "monitor:security": "node scripts/security-monitor.js",
    "clean": "rimraf .next node_modules dist coverage",
    "doctor": "yarn check --verify-tree && yarn check --integrity",
    "deploy:vercel": "node scripts/deploy-vercel.js",
    "deploy:vercel:preview": "node scripts/deploy-vercel.js --preview",
    "deploy:vercel:simple": "node scripts/deploy-vercel-simple.js",
    "deploy:vercel:simple:preview": "node scripts/deploy-vercel-simple.js --preview",
    "vercel:build": "next build",
    "vercel:dev": "vercel dev",
    "vercel:pull": "vercel env pull .env.local",
    "test:local": "node scripts/test-local-deployment.js",
    "test:local:full": "npm run dev & sleep 10 && npm run test:local && kill %1",
    "start:clean": "node scripts/start-clean.js",
    "dev:clean": "node scripts/start-clean.js",
    "build:production": "node scripts/build-production.js",
    "setup:database": "node scripts/setup-production-db.js",
    "deploy:production": "npm run build:production && npm run deploy:vercel:simple",
    "validate:production": "npm run setup:database && npm run build:production",
    "deploy:final": "node scripts/deploy-final.js",
    "checklist": "node scripts/production-deployment-checklist.js"
  },
